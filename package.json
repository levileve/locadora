{
  "name": "teste-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf build; mkdir build && babel --ignore node_modules -d ./build ./",
    "start": "npm run build && npm run && cp ./.env ./build/.env && cd ./build/ && node index.js",
    "start-dev": "npx nodemon -- -r esm -r dotenv/config index.js",
    "migration-generate": "npx sequelize-cli migration:generate --name",
    "migration": "npx sequelize-cli db:migrate",
    "undo-migration": "npx sequelize-cli db:migrate:undo --name",
    "seed-generate": "npx sequelize-cli seed:generate --name",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo --name",
    "update-db": "npm run migration && npm run seed",
    "lint": "npx eslint -c .eslintrc --ext .js ./src --fix",
    "precommit": "npm run lint"
  },
  "author": "Levi de Souza dos Santos",
  "private": true,
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.19.0",
    "cjs": "0.0.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.2.0",
    "fs": "0.0.1-security",
    "helmet": "^3.18.0",
    "http-status": "^1.3.2",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "method-override": "^3.0.0",
    "moment-timezone": "^0.5.25",
    "nodemon": "^2.0.4",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "request-ip": "^2.1.3",
    "sequelize": "^5.22.3",
    "uuid": "^3.3.2",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^2.3.0"
  }
}
